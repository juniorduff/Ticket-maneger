{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "User/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "User/user": "<rootDir>/../libs/user/src",
    "exercise/exercise/(.*)": "<rootDir>/../libs/exercise/src/$1",
    "exercise/exercise": "<rootDir>/../libs/exercise/src"
  }
}